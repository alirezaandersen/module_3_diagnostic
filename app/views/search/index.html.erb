<h1>Stations Near By</h1>
<% @stations.each_with_index do |station, ind| %>
  <div id="station-<%= ind %>">
    <h4>Name: <%= station.name %></h1>
    <h6>Address: <%= station.street_address %></h3>
    <h6>Fuel types: <%= station.fuel_types %></h3>
    <h6>Distance: <%= station.distance %> miles</h3>
    <h6>Access times: <%= station.access_times %></h3>
  </div>
<% end %>

<% will_paginate @stations %>
